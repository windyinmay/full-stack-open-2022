{
	"compilerOptions": {
		"target": "ES6",
		"outDir": "./build/",
		// "rootDir": "src",
		// "baseUrl": "./src/",
		"module": "commonjs",
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"esModuleInterop": true,
		"resolveJsonModule": true
	}
}
